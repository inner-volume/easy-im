{
  "name": "pkg6/easy-im",
  "license": "MIT",
  "description": "非官方即时通信 IM，支持腾讯IM，环信IM，极光IM，融云IM，网易云信IM等",
  "homepage": "https://github.com/pkg6/easy-im",
  "keywords": [
    "easy-im",
    "IM",
    "腾讯IM",
    "环信IM",
    "极光IM",
    "融云IM",
    "网易云信IM"
  ],
  "authors": [
    {
      "name": "zzqqw",
      "email": "zhiqiangwang@aliyun.com"
    }
  ],
  "require": {
    "php": ">7.4",
    "ext-zlib": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "psr/cache": "~1.0|^2.0",
    "psr/log": "^1.0.1 || ^2.0",
    "psr/simple-cache": "^1.0|^2.0",
    "guzzlehttp/guzzle": "^6|^7",
    "pimple/pimple": "^3.0",
    "pkg6/cache": "^0.1",
    "pkg6/log": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Pkg6\\easyIm\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9",
    "mockery/mockery": "^1.2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Pkg6\\easyIm\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fresh": [
      "rm -rf vendor composer.lock",
      "composer install"
    ],
    "tests": [
      "php ./vendor/bin/phpunit"
    ]
  }
}
