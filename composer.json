{
  "name": "whereof/easy-im",
  "license": "MIT",
  "description": "非官方即时通信 IM，支持腾讯IM，环信IM，极光IM，融云IM，网易云信IM等",
  "homepage": "https://github.com/tp5er/easy-ims",
  "keywords": [
    "easy-im",
    "IM",
    "腾讯IM",
    "环信IM",
    "极光IM",
    "融云IM",
    "网易云信IM"
  ],
  "authors": [
    {
      "name": "qmister",
      "email": "tp5er@qq.com"
    }
  ],
  "require": {
    "php": ">7.0",
    "ext-zlib": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6|^7",
    "pimple/pimple": "^3.0",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "whereof\\easyIm\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9",
    "mockery/mockery": "^1.2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "whereof\\easyIm\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fresh": [
      "rm -rf vendor composer.lock",
      "composer install"
    ],
    "tests": [
      "php ./vendor/bin/phpunit"
    ]
  }
}
